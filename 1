#include<stdio.h>
#include<stdlib.h>
void merge(int nums1[],int m, int nums2[],int n){
   int i = m + n;
   int l=m+n;
        while (m > 0 && n > 0) 
        {
            if (nums1[m - 1] > nums2[n - 1])
            {
                nums1[i-1]=nums1[m-1];
                m--;
            } else {
                nums1[i - 1] = nums2[n - 1];
                n--;
            }
            i--;
        }

        while (n > 0) {
            nums1[i - 1] = nums2[n - 1];
            n--;
            i--;
        }
        for(int i=0;i<l;i++)
         printf("%d ",nums1[i]);
}
int main()
{
        int m=3,n=3,i;
        int nums1[6]={1,2,3,0,0,0};
        int nums2[3]={2,5,6};
        merge(nums1,m,nums2,n);
}


